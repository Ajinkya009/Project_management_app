    var ProjectService = /*#__PURE__*/function () {
      function ProjectService(router, http) {
        _classCallCheck(this, ProjectService);

        this.router = router;
        this.http = http;
      }

      _createClass(ProjectService, [{
        key: "fetchProjectDetails",
        value: function fetchProjectDetails(projectId) {
          var auth_token = localStorage.getItem('auth_token');
          return this.http.get("/api/project/".concat(projectId), {
            headers: {
              Authorization: "Token ".concat(auth_token)
            }
          });
        }
      }, {
        key: "createNewTask",
        value: function createNewTask(name, description, startDate, endDate, assignee, projectId) {
          var auth_token = localStorage.getItem('auth_token');
          return this.http.post("/api/project/".concat(projectId, "/task/"), {
            name: name,
            description: description,
            startDate: startDate,
            endDate: endDate,
            assignee: assignee,
            project: projectId
          }, {
            headers: {
              Authorization: "Token ".concat(auth_token)
            }
          });
        }
      }, {
        key: "updateProject",
        value: function updateProject(projectData) {
          var auth_token = localStorage.getItem('auth_token');
          return this.http.put("/api/project/".concat(projectData.id, "/"), projectData, {
            headers: {
              Authorization: "Token ".concat(auth_token)
            }
          });
        }
      }]);

      return ProjectService;
    }();

    ProjectService.ɵfac = function ProjectService_Factory(t) {
