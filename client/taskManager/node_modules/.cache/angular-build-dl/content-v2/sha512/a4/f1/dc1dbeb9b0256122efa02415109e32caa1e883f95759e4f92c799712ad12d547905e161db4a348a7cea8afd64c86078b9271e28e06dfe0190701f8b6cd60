      }

      _createClass(DashboardService, [{
        key: "fetchAllProjects",
        value: function fetchAllProjects() {
          var auth_token = localStorage.getItem('auth_token');
          return this.http.get('/api/project/', {
            headers: {
              Authorization: "Token ".concat(auth_token)
            }
          });
        }
      }, {
        key: "createNewProject",
        value: function createNewProject(name, description, startDate, endDate) {
          var auth_token = localStorage.getItem('auth_token');
          return this.http.post('/api/project/', {
            name: name,
            description: description,
            startDate: startDate,
            endDate: endDate
          }, {
            headers: {
              Authorization: "Token ".concat(auth_token)
            }
          });
        }
      }]);

      return DashboardService;
    }();

